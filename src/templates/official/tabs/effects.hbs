    <ul class="effects-list">
      {{#each actor.effects}}
      <li
        class="item flexrow {{#unless data.origin}}active-effect{{/unless}} {{#if data.disabled}}disabled-effect{{/if}}"
        data-effect-id="{{id}}"
      >
        <img src="{{data.icon}}" alt="{{data.label}}" class="effect-icon" />
        {{#if origin}}
        <button
          type="button"
          class="effect-label {{#if data.origin}}effect-action{{/if}}"
          data-action="open-origin"
        >
          {{label}}
        </button>
        {{else}}
        <span class="effect-label">{{data.label}}</span>
        {{/if}}
        <span class="effect-controls">
          <button type="button" class="effect-action" data-action="toggle" data-toggle="disabled">
            <i class="fas fa-power-off"></i>
          </button>
          <button 
          type="button" 
          class="effect-action {{#unless data.flags.swade.favorite}}item-unequipped{{/unless}}" 
          data-action="toggle"
          data-toggle="flags.swade.favorite"
          title="{{localize "SWADE.Favorite"}}"
          >
            <i class="fas fa-star"></i>
          </button>
          <button type="button" class="effect-action" data-action="edit">
            <i class="fas fa-edit"></i>
          </button>
          <a class="effect-action" data-action="delete">
            <i class="fas fa-trash"></i>
          </a>
        </span>
      </li>
      {{/each}}
    </ul>