{
  "private": true,
  "name": "swade",
  "version": "0.21.4",
  "description": "An implementation of the Savage Worlds Adventure Edition game system for Foundry Virtual Tabletop (http://foundryvtt.com). This work is permitted under the Pinnacle Entertainment Group Fan License (https://www.peginc.com/licensing)",
  "author": "FloRad",
  "license": "Apache License, Version 2.0",
  "scripts": {
    "package": "gulp package",
    "build": "gulp build && gulp link",
    "build:watch": "gulp watch",
    "clean": "gulp clean && gulp link --clean",
    "update": "npm uninstall foundry-vtt-types && npm install --save-dev github:League-of-Foundry-Developers/foundry-vtt-types#foundry-0.8.x",
    "check:lint": "eslint --ext .ts,.js ./src",
    "check:types": "tsc --noEmit",
    "prepare": "husky install",
    "pretty-staged": "pretty-quick --staged --pattern '**/*.!(hbs)'"
  },
  "devDependencies": {
    "@league-of-foundry-developers/foundry-vtt-types": "^0.8.9-7",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "archiver": "^5.3.0",
    "chalk": "^4.1.2",
    "eslint": "^8.2.0",
    "fs-extra": "^10.0.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-dart-sass": "^1.0.2",
    "gulp-git": "^2.10.1",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-yaml": "^2.0.4",
    "husky": "^7.0.2",
    "json-stringify-pretty-compact": "^3.0.0",
    "merge-stream": "^2.0.0",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "sass": "^1.32.8",
    "typescript": "^4.4.3",
    "yargs": "^17.2.1"
  }
}
