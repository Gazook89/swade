{
  "private": true,
  "name": "swade",
  "version": "1.0.3",
  "description": "An implementation of the Savage Worlds Adventure Edition game system for Foundry Virtual Tabletop (http://foundryvtt.com). This work is permitted under the Pinnacle Entertainment Group Fan License (https://www.peginc.com/licensing)",
  "author": "FloRad",
  "license": "https://gitlab.com/peginc/swade/-/blob/master/LICENSE.md",
  "homepage": "https://gitlab.com/peginc/swade",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/peginc/swade"
  },
  "bugs": {
    "url": "https://gitlab.com/peginc/swade/-/issues"
  },
  "scripts": {
    "build:watch": "rollup -cw",
    "build": "rollup -c && gulp link",
    "build:packs": "gulp packs",
    "package": "gulp package",
    "clean": "rimraf dist && gulp link --clean",
    "check:lint": "eslint --ext .ts,.js ./src",
    "check:types": "tsc --noEmit",
    "prepare": "husky install",
    "pretty-staged": "pretty-quick --staged --pattern '**/*.!(hbs)'"
  },
  "devDependencies": {
    "@guanghechen/rollup-plugin-copy": "^1.8.6",
    "@league-of-foundry-developers/foundry-vtt-types": "^9.238.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "archiver": "^5.3.0",
    "chalk": "^4.1.2",
    "eslint": "^8.2.0",
    "eslint-plugin-deprecation": "^1.3.2",
    "fs-extra": "^10.0.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-git": "^2.10.1",
    "gulp-rename": "^2.0.0",
    "gulp-yaml": "^2.0.4",
    "husky": "^7.0.2",
    "js-yaml": "^4.1.0",
    "json-stringify-pretty-compact": "^3.0.0",
    "merge-stream": "^2.0.0",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.67.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-styles": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.32.8",
    "typescript": "^4.5.4",
    "yargs": "^17.2.1"
  }
}
